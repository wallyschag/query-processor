{
  "op": "Project",
  "exprs": [
    { "as": "name", "expr": { "col": "c.name" } },
    { "as": "discounted_balance", "expr": {
        "op": "MUL",
        "left": { "col": "c.balance" },
        "right": { "const": 0.9, "type": "float" }
      } }
  ],
  "input": {
    "op": "Select",
    "predicate": {
      "op": "EQ",
      "left": { "col": "c.country" },
      "right": { "const": "USA", "type": "string" }
    },
    "input": {
      "op": "Scan",
      "table": "./data/example1/customers.csv",
      "as": "c"
    }
  }
}
