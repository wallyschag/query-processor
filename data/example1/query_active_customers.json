{
  "op": "Project",
  "exprs": [
    { "as": "customer_id", "expr": { "col": "c.customer_id" } },
    { "as": "name", "expr": { "col": "c.name" } },
    { "as": "country", "expr": { "col": "c.country" } }
  ],
  "input": {
    "op": "Select",
    "predicate": {
      "op": "EQ",
      "left": { "col": "c.is_active" },
      "right": { "const": true, "type": "bool" }
    },
    "input": {
      "op": "Scan",
      "table": "./data/example1/customers.csv",
      "as": "c"
    }
  }
}
