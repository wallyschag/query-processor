{
  "op": "Project",
  "exprs": [
    { "as": "name", "expr": { "col": "c.name" } },
    { "as": "balance", "expr": { "col": "c.balance" } }
  ],
  "input": {
    "op": "Select",
    "predicate": {
      "op": "GT",
      "left": { "col": "c.balance" },
      "right": { "const": 100.0, "type": "float" }
    },
    "input": {
      "op": "Scan",
      "table": "./data/example1/customers.csv",
      "as": "c"
    }
  }
}
